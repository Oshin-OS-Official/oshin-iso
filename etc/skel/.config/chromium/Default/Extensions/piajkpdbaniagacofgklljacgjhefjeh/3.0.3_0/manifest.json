{
   "action": {
      "default_icon": {
         "19": "img/icon19.png",
         "38": "img/icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Advanced Profanity Filter"
   },
   "author": "FrostCo",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "webFilter.js" ],
      "match_about_blank": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "Advanced Profanity Filter helps to clean up bad language on the websites you and your family visit.",
   "host_permissions": [ "https://*.crunchyroll.com/", "https://*.vrv.co/" ],
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Advanced Profanity Filter",
   "options_ui": {
      "open_in_tab": true,
      "page": "optionPage.html"
   },
   "permissions": [ "contextMenus", "notifications", "scripting", "storage", "tabs" ],
   "short_name": "APF",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0.3",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "audio/*.mp3" ]
   } ]
}
